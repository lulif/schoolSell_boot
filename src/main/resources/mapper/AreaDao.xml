<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdxx.dao.AreaDao">
	<select id="queryArea" resultType="com.gdxx.entity.Area">
		select
		area_id,area_name,priority,create_time,last_edit_time from area order
		by priority DESC
	</select>
</mapper>